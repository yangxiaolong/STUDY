<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bjpowernode.dao.StudentMapper">
    <select id="list" resultType="bjpowernode.domain.Student">
        select * from user
    </select>
    <insert id="insertStudent">
        insert into user value (#{id},#{name},#{pwd},#{age})
    </insert>

</mapper>